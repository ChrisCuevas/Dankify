{
	"name": "Dankify",
	"description": "The dankest chrome extension ever.",
	"version": "1.0",
	"manifest_version": 2,
	"browser_action": {
   		 "default_icon": "icon.png"
  	},
	"permissions": [
    		"tabs", "http://*/*"
  	],
	"content_scripts": [
		{
			"matches": ["http://*/*"],
			"js": ["dank.js"],
			"run_at": "document_start"
		}
	]
}
	
